{
	"name": "dokaka-site-version-2024",
	"version": "2.0.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"cbuild": "rm -rf build && vite build",
		"preview": "vite preview",
		"wgin": "wrangler login",
		"wgout": "wrangler logout",
		"wgup": "wrangler pages deploy build --project-name yourprjname",
		"audput": "wrangler r2 object put remotebucketname/audios/\"$file\" --file ./r2-bucket-local/localmediadir/audios/\"$file\" --jurisdiction eu",
		"vidput": "wrangler r2 object put remotebucketname/videos/\"$file\" --file ./r2-bucket-local/localmediadir/videos/\"$file\" --jurisdiction eu",
		"picput": "wrangler r2 object put remotebucketname/pictures/\"$file\" --file ./r2-bucket-local/localmediadir/pictures/\"$file\" --jurisdiction eu",
		"lint": "prettier --ignore-path .gitignore --check -- plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"mdsvex": "^0.11.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.1.0",
		"svelte": "^4.2.7",
		"svelte-preprocess": "^5.0.3",
		"vite": "^5.0.12",
		"wrangler": "^3.62.0"
	},
	"type": "module",
	"dependencies": {
		"chart.js": "^4.4.1",
		"svelte-chartjs": "^3.1.5"
	},
	"packageManager": "pnpm@9.4.0"
}
